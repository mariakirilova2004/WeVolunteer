@using WeVolunteer.Core.Constants

@if(TempData[MessageConstant.ErrorMessage] != null)
{
    <script>
        $(document).ready(function() { toastr.error('@TempData[MessageConstant.ErrorMessage].ToString()'); });
    </script>
}

@if(TempData[MessageConstant.WarningMessage] != null)
{
    <script>
        $(document).ready(function() { toastr.warning('@TempData[MessageConstant.WarningMessage].ToString()'); });
    </script>
}

@if(TempData[MessageConstant.SuccessMessage] != null)
{
    <script>
        $(document).ready(function() { toastr.success('@TempData[MessageConstant.SuccessMessage].ToString()'); });
    </script>
}